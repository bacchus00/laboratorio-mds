        "id": "q0VCVJI8F0ga"
      },
      "source": [
        "**Respuesta:**"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 16,
      "metadata": {
        "id": "ay080DunHcOS"
      },
      "outputs": [],
      "source": [
        "from sklearn.compose import make_column_transformer\n",
        "\n",
        "atributos_de_interes = [\n",
        "    'intelligence_score',\n",
        "    'strength_score',\n",
        "    'speed_score',\n",
        "    'durability_score',\n",
        "    'power_score',\n",
        "    'combat_score'\n",
        "]\n",
        "\n",
        "t_bow = ('t_bow', CountVectorizer(tokenizer=StemmerTokenizer(), ngram_range=(1,2)), 'history_text')\n",
        "t_atr = ('t_atr', MinMaxScaler(), atributos_de_interes)\n",
        "\n",
        "col_trans = ColumnTransformer([t_bow, t_atr])"
      ]
    },
